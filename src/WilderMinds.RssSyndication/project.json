{
  "version": "1.0.1-*",
  "description": ".NET Core Project for building RSS Feeds",
  "authors": [ "Shawn Wildermuth" ],
  "packOptions": {
    "tags": [ "aspnetcore", "netcore", "rss", "syndication" ],
    "repository": {
      "type": "git",
      "url": "git://github.com/shawnwildermuth/RssSyndication"
    },
    "projectUrl": "https://github.com/shawnwildermuth/RssSyndication",
    "licenseUrl": "https://github.com/shawnwildermuth/RssSyndication/blob/master/License.md"
  },
  "buildOptions": {
    "warningsAsErrors": true,
    "xmlDoc": false
  },

  "dependencies": {
    "Microsoft.NETCore.App": {
      "type": "platform",
      "version": "1.0.0-rc2-*"
    }
  },

  "frameworks": {
    "netcoreapp1.0": {
      "imports": [ "dotnet5.4" ],
      "dependencies": {
        "Microsoft.CSharp": "4.0.1-rc2-*",
        "System.Collections": "4.0.11-rc2-*",
        "System.Linq": "4.1.0-rc2-*",
        "System.Runtime": "4.1.0-rc2-*",
        "System.Threading": "4.0.11-rc2-*",
        "System.Xml.XDocument": "4.0.11-rc2-*"
      }
    }
  }
}
